{
  "name": "database_document",
  "main": "./main/index.ts",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@ant-design/icons": "^4.6.4",
    "@craco/craco": "^6.2.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "antd": "^4.16.13",
    "classnames": "^2.3.1",
    "craco-antd": "^1.19.0",
    "electron-is-dev": "^2.0.0",
    "eslint": "^7.32.0",
    "mysql2": "^2.3.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "sequelize": "^6.6.5",
    "typescript": "^4.1.2",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start:render": "cross-env BROWSER=none craco start",
    "start:build:main": "cross-env NODE_ENV=development ts-node --project ./config/tsconfig.json ./config/start.ts",
    "start:main": "wait-on http://localhost:3000 && nodemon --watch ./build --exec electron ./build/main.js",
    "start": "npm-run-all --parallel start:*",
    "build:render": "craco build",
    "build:main": "cross-env NODE_ENV=production ts-node --project ./config/tsconfig.json ./config/start.ts",
    "build": "npm-run-all build:*",
    "lint": "eslint --fix",
    "test": "craco test",
    "package-all": "npm run build && electron-builder build -mwl",
    "package-mac": "npm run  build && electron-builder build --mac",
    "package-linux": "npm run  build && electron-builder build --linux",
    "package-win": "npm run  build && electron-builder build --win --x64"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/sequelize": "^4.28.10",
    "concurrently": "^6.2.1",
    "craco-less": "^1.20.0",
    "cross-env": "^7.0.3",
    "electron": "^14.0.0",
    "electron-builder": "^22.11.7",
    "eslint": "^7.32.0",
    "npm-run-all": "^4.1.5",
    "ts-loader": "^8.2.0",
    "ts-node": "^10.2.1",
    "wait-on": "^6.0.0"
  }
